<?xml version="1.0" encoding="UTF-8"?>

<DomainModel xmlns="http://ae.ossys.com"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://ae.ossys.com http://ae.ossys.com/domainmodel.xsd"
             name="Gmail Password">



<!-- User -->
	<BusinessObject name="User" comment="" concurrent="true" atomic="true" auditable="true">
					
		<!-- EMail -->
		<Attribute name="EMail" persistent="true" type="TEXT" minsize="1" maxsize="30" comment="The user's email."
					characterset="utf-8" required="true" default="" validator="E-Mail" />
	
	</BusinessObject>
		
</DomainModel>
